{
  "name": "flipflip-server",
  "version": "4.0.0",
  "license": "MIT",
  "bin": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && webpack",
    "package": "yarn build && rimraf pkg && yarn pkg --compress Brotli . && yarn package:win",
    "package:win": "tsc package/win.ts --skipLibCheck --target esnext --module nodenext --moduleResolution nodenext && yarn run-func package/win.js windowsPostBuild",
    "start": "yarn build && node dist/index.js",
    "lint": "eslint --fix --ignore-path .eslintignore --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "devDependencies": {
    "@types/bcryptjs": "^2",
    "@types/body-parser": "^1",
    "@types/debug": "^4",
    "@types/eslint": "^8",
    "@types/express": "4.17.1",
    "@types/express-session": "^1",
    "@types/file-url": "^3.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/get-folder-size": "^3.0.4",
    "@types/js-yaml": "^4",
    "@types/node": "^18.10.1",
    "@types/oauth": "^0.9.4",
    "@types/recursive-readdir": "^2.2.4",
    "@types/twitter": "^1.7.4",
    "@types/ws": "^8",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "node-loader": "^2.0.0",
    "pkg": "^5.8.1",
    "prettier": "^3.1.0",
    "resedit": "^2.0.0",
    "rimraf": "^5.0.5",
    "run-func": "^3.0.0",
    "ts-loader": "^9.5.1",
    "tslib": "^2.6.2",
    "typescript": "*",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@httptoolkit/httpolyglot": "^2.2.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "bufferutil": "^4.0.1",
    "debug": "^4.3.4",
    "express": "4.17.1",
    "express-session": "^1.17.3",
    "file-url": "^4.0.0",
    "flipflip-common": "link:./../common",
    "font-list": "^1.5.1",
    "fs-extra": "^11.1.1",
    "generate-password": "^1.7.1",
    "get-folder-size": "^4.0.0",
    "imgur": "^2.3.0",
    "instagram-private-api": "^1.45.3",
    "js-yaml": "^4.1.0",
    "moment": "^2.29.4",
    "music-metadata": "^7.13.5",
    "oauth": "^0.10.0",
    "popups-file-dialog": "^1.6.2",
    "re2": "^1.20.9",
    "recursive-readdir": "^2.2.3",
    "selfsigned": "^2.4.1",
    "snoowrap": "^1.23.0",
    "supports-color": "^8.1.1",
    "system-font-families": "^0.6.0",
    "tumblr.js": "^4.0.1",
    "twitter": "^1.7.1",
    "unique-username-generator": "^1.2.0",
    "utf-8-validate": "^6.0.3",
    "wretch": "^2.7.0",
    "ws": "^8.14.2"
  },
  "pkg": {
    "assets": [
      "assets/*",
      "public/*",
      "public/**/*"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "pkg"
  },
  "packageManager": "yarn@4.0.2"
}
